{
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}
